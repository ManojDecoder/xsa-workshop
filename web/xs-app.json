{
	"welcomeFile": "index.html",
	"authenticationMethod": "route",
	"routes": [{
		"source": "/rest/(.*)",
		"destination": "nodejs",
		"authenticationType": "xsuaa"
	}, {
		"source": "(.*)(.xsjs)",
		"destination": "nodejs",
		"authenticationType": "xsuaa"
	}, {
		"source": "(.*)(.xsodata)",
		"destination": "nodejs",
		"authenticationType": "xsuaa"
	}]
}