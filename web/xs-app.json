{
	"welcomeFile" : "index.html",
	"routes" : [{
			"source" : "/sap/ui5/1(.*)",
			"target" : "$1",
			"destination" : "ui5"
		}, {
			"source" : "(workshop.*)(.xsjs)",
			"destination" : "xsjs"
		}, {
			"source" : "(workshop.*)(.xsodata)",
			"destination" : "xsjs"
		}, {
			"source" : "/rest/(.*)",
			"target" : "$1",
			"destination" : "nodejs"
		}
	]
}
