{
	"welcomeFile": "index.html",
	"authenticationMethod": "route",
	"routes" : [
		{
			"source" : "(workshop.*)(.xsjs)",
			"destination" : "xsjs"
		}, {
			"source" : "(workshop.*)(.xsodata)",
			"destination" : "xsjs"
		}, {
			"source" : "/rest/(.*)",
			"target" : "$1",
			"destination" : "nodejs"
		}
	]
}